.likes{
  font-size:32rpx;
  text-align: center;
  padding: 50rpx 0 28rpx;
}
/*列表*/
.prelist{
  width:100%;
  padding:0 30rpx;
  display:flex;
  justify-content: space-between;
  flex-wrap: wrap;
}
/*单个列表*/
.prepanle1{
  width:330rpx;
  height: 460rpx;
  background:#fff;
  margin-bottom:30rpx;
}
/*单个列表头部*/
.panlehead{
  padding: 20rpx;
  width:100%;
  display: flex;
  justify-content: space-between;
}
/*类型*/
.ly{
  font-size:22rpx;
  background: #f7f7f7;
  padding:9rpx 14rpx;
}
.prepanle1>image{
  width:100%;
  height:240rpx;
}
/*文字*/
.panletext{
  padding:32rpx 21rpx 0;
  line-height:43rpx;
  font-size:26rpx;
  color:#999;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}