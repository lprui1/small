<view class="perlike">
  <view class="likes">喜欢</view>
  <view class="prelist">
    <view class="prepanle1" wx:for="{{Prelist}}" wx:key="index">
      <view class="panlehead">
        <text class="ly">{{item.type}}</text>
        <f-like count="{{item.fav_nums}}"/>
      </view>
      <image src="{{item.image}}"></image>
      <view class="panletext">
        {{item.content}}
      </view>
    </view>
  </view>
</view>
